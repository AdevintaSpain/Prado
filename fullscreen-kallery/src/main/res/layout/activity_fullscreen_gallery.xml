<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.schibsted.spain.fullscreenkallery.FullscreenGalleryActivity"
    >



  <TextView
      android:id="@+id/pagerIndicatorNumber"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:textAppearance="@style/gallery_pagerIndicator_text"
      style="@style/gallery_pagerIndicator"
      />

  <ImageView
      android:id="@+id/galleryCloseIcon"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      style="@style/gallery_close_icon"
      />

</android.support.constraint.ConstraintLayout>
